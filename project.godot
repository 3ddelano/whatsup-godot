config_version=5

[application]

config/name="WhatsupGodot"
config/description="A cross-platform, instant messaging application created using Godot Game Engine."
run/main_scene="res://pages/app/App.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/bg_color=Color(0.12549, 0.12549, 0.12549, 1)
boot_splash/image="res://assets/icons/splash.png"
boot_splash/fullsize=false
config/icon="res://assets/icons/splash.png"

[autoload]

Store="*res://autoloads/Store.gd"
Tweens="*res://autoloads/Tweens.gd"
Seed="*res://autoloads/Seed.gd"

[display]

window/size/viewport_width=1366
window/size/viewport_height=768
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray()

